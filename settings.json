{
  "vim.enableNeovim": true,
  "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "ctrl+["
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "editor.fontSize": 18,
  "editor.fontFamily": "\"Monego\", \"Utatane\"",
  "editor.rulers": [
    80,
    100,
    120
  ],
  "editor.renderWhitespace": "boundary",
  "indentRainbow.colors": [
    "rgba(255,127,127,0.15)",
    "rgba(255,127,255,0.15)",
    "rgba(127,127,255,0.15)",
    "rgba(127,191,255,0.15)"
  ],
  "indentRainbow.errorColor": "rgba(255,100,100,0.6)",
  "workbench.startupEditor": "newUntitledFile",
  "[markdown]": {
    "editor.wordWrap": "on",
    "editor.quickSuggestions": true
  },
  "markdownlint.config": {
    "default": true,
    "MD007": {
      "indent": 2
    },
    "MD009": false
  },
  "python.linting.enabled": true,
  "python.pythonPath": "/usr/bin/python3",
  "python.linting.pylintPath": "/usr/local/bin/pylint",
  "python.testing.unittestEnabled": true,
  "python.testing.unittestArgs": [
    "-v",
    "-s",
    "./tests",
    "*test*.py"
  ],
  "plantuml.exportFormat": "png",
  "window.zoomLevel": 0,
  "terminal.integrated.rendererType": "dom",
  "breadcrumbs.enabled": true,
  "window.titleBarStyle": "custom",
  "todohighlight.include": [
    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx",
    "**/*.html",
    "**/*.php",
    "**/*.css",
    "**/*.scss",
    "**/*.go"
  ],
  "todohighlight.exclude": [
    "**/node_modules/**",
    "**/bower_components/**",
    "**/dist/**",
    "**/vendor/**",
    "**/build/**",
    "**/.vscode/**",
    "**/.github/**",
    "**/_output/**",
    "**/*.min.*",
    "**/*.map",
    "**/.next/**"
  ],
  "todohighlight.keywords": [
    {
      "text": "TODO:",
      "color": "#0000ff",
      "backgroundColor": "#ddddff",
      "overviewRulerColor": "#ddddff",
      "isWholeLine": true
    },
    {
      "text": "DEBUG:",
      "color": "#ffffff",
      "backgroundColor": "#555555",
      "overviewRulerColor": "#555555",
      "isWholeLine": true
    },
    {
      "text": "NOTE:",
      "color": "#00aa00",
      "backgroundColor": "#ddffdd",
      "isWholeLine": false
    },
    {
      "text": "FIXME:",
      "color": "#ffffff",
      "backgroundColor": "#ff99cb",
      "overviewRulerColor": "#ff99cb",
      "isWholeLine": true
    },
    {
      "text": "HACK:",
      "color": "#ff00ff",
      "backgroundColor": "#ccbbff",
      "overviewRulerColor": "#ccbbff",
      "isWholeLine": true
    }
  ],
  "powermode.enabled": true,
  "powermode.enableShake": false,
  "powermode.presets": "fireworks",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.activityBar.visible": true,
  "asciidoc.preview.fontFamily": "'Utatane'",
  "markdown.preview.fontFamily": "'Utatane'",
  "debug.console.fontFamily": "'Utatane'",
  "liveshare.featureSet": "insiders",
  "cSpell.enabledLanguageIds": [
    "Log",
    "asciidoc",
    "bat",
    "c",
    "clojure",
    "coffeescript",
    "cpp",
    "csharp",
    "css",
    "csv",
    "dart",
    "diff",
    "dockerfile",
    "elm",
    "git-commit",
    "go",
    "groovy",
    "handlebars",
    "html",
    "ignore",
    "ini",
    "jade",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "kotlin",
    "latex",
    "less",
    "log",
    "lua",
    "makefile",
    "markdown",
    "objective-c",
    "objective-cpp",
    "perl",
    "perl6",
    "php",
    "plaintext",
    "powershell",
    "pug",
    "python",
    "r",
    "restructuredtext",
    "ruby",
    "rust",
    "scala",
    "scss",
    "shellscript",
    "sql",
    "swift",
    "text",
    "todo",
    "toml",
    "typescript",
    "typescriptreact",
    "xml",
    "xsl",
    "yaml",
    "yml"
  ],
  "sync.gist": "df111e7a06effa548f3a2162accd0374",
  "workbench.colorTheme": "Synthwave x Fluoromachine",
  "vscode_custom_css.imports": [
    "file:///Users/fsuser/synthwave84.css",
    "/Users/fsuser/.vscode/extensions/webrender.synthwave-x-fluoromachine-0.0.2/synthwave-x-fluoromachine.css"
  ],
  "cSpell.ignoreRegExpList": [
    "[０-９Ａ-Ｚａ-ｚぁ-んァ-ヶ亜-熙纊-黑]+"
  ],
  "editor.renderControlCharacters": true,
  "terminal.integrated.fontSize": 18,
  "terminal.integrated.fontFamily": "CodeMForPowerline-Regular",
  "sync.forceUpload": true,
  "sync.autoDownload": true,
  "sync.autoUpload": true,
  "dart.openDevTools": "flutter",
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "ruby.lint": {
    "rubocop": {
      "useBundler": true
    },
  },
  "ruby.format": "rubocop",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "C_Cpp.clang_format_style": "{ BasedOnStyle: LLVM, BreakBeforeBraces: Attach, SpaceBeforeParens: Never, IndentWidth: 4 }",
  "C_Cpp.updateChannel": "Insiders"
}